local player = game.Players.LocalPlayer

-- Load the Teleport DS library
print("Loading Teleport DS library...")
local TeleportDS = loadstring(game:HttpGet('https://raw.githubusercontent.com/shlexware/Orion/main/source'))()

-- Create the main window
print("Creating main window...")
local Window = TeleportDS:MakeWindow({Name = "Teleport DS", HidePremium = false, SaveConfig = true, ConfigFolder = "TeleportDS"})

-- Create a new tab
print("Creating tab...")
local Tab = Window:MakeTab({
    Name = "Teleports",
    Icon = "rbxassetid://4483345998",
    PremiumOnly = false
})

-- Add buttons for teleportation
print("Adding buttons...")
Tab:AddButton({
    Name = "Default Map 2018-2019",
    Callback = function()
        print("Teleporting to Default Map 2018-2019")
        local teleportService = game:GetService("TeleportService")
        teleportService:Teleport(15918631033, player)
    end    
})

Tab:AddButton({
    Name = "Gallus Island",
    Callback = function()
        print("Teleporting to Gallus Island")
        local teleportService = game:GetService("TeleportService")
        teleportService:Teleport(794299040, player)
    end    
})

Tab:AddButton({
    Name = "Homelands",
    Callback = function()
        print("Teleporting to Homelands")
        local teleportService = game:GetService("TeleportService")
        teleportService:Teleport(16211666354, player)
    end    
})

-- Initialize the GUI
print("Initializing GUI...")
TeleportDS:Init()
print("Script executed successfully!")
